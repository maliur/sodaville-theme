    "publisher": "Maliur",
    "version": "0.1.0",
    "type": "module",
    "engines": {
        "vscode": "^1.49.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/maliur/sodaville-theme"
    },
    "bugs": {
